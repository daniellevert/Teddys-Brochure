{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
        ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-unused-vars": "warn",
        "no-console": "off",
		"indent": [2, "tab"],
        "no-tabs": 0,
        "no-plusplus": "off",
        "max-len": ["error", { "code": 150 }]
    }
}
